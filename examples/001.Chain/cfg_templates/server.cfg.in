[general]
debug_mode=true
debug_dump_stream_headers=false
wallets_default=default
# seed mode. WARNING. Used true only when you start the new network
seed_mode=$seed_mode
auto_online=true


[conserver]
enabled=true
listen_unix_socket_path=$listen_unix_socket_path


[server]
enabled=true
listen_address=127.0.0.1
listen_port_tcp=$node_port
server_threads=4
events_threads=0
events_timeout=0
stream_dump_headers=false
stream_ctl_keytype=oaes
stream_ctl_keytype_size=32


[cdb]
enabled=false
servers_list_enabled=true
servers_list_networks=[$net_name]


[resources]
#   0 means auto detect
threads_cnt=0
pid_path=$pid_path
log_file=$log_file
wallets_path=$wallets_path
ca_folders=$ca_folders
dap_global_db_path=$dap_global_db_path
dap_global_db_driver=cdb
