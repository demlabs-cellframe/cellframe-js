# General section
[general]
id=0x0000000000001234
name=devnet
type=development
# Possible values: light, full, archive, master, root
node-role=$node_role
node_addr_type=static
node-addr=$node_addr
gdb_groups_prefix=devnet
# node addr exired time in hours (168h=1w 720h=1m 8760h=1y), by default 720h(1week)
node-addr-expired=168

seed_nodes_hostnames=$hostnames
seed_nodes_aliases=$aliases
seed_nodes_addrs=$addrs
seed_nodes_port=$ports

#[role-master]
#proc_chains=[0x00000001]

[dag-poa]
events-sign-cert=$node_cert_poa

[dag-pos]
events-sign-wallet=$node_wallet
