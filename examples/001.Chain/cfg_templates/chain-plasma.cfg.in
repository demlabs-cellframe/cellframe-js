[chain]
id=0x0000000000000001
name=plasma
load_priority=2
# Possible values [token, emission, transaction] 
datum_types=[transaction,ca]
consensus=dag_pos

[dag]
is_single_line=false
is_celled=true
is_add_directly=true
datum_add_hashes_count=1
#is_static_genesis_event=$plasma_is_static_genesis_event
#static_genesis_event=$plasma_static_genesis_event

# Proof-of-State consensus config
[dag-pos]
# Tokens hold and hold_value must be equel size
# KELT - test token, KEL - main tokem
tokens_hold=$tokens_hold
# 1000 coins for both
tokens_hold_value=$tokens__hold_value
# Confirmations minimum
confirmations_minimum=1

[files]
storage_dir=$plasma_storage_dir

